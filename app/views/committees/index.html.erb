<h2>Komitety wyborcze</h2>

<div class = "container">
  <table class = "table">
    <thead>
    <tr>
        <th>Nazwa</th>
        <th>Logo</th>
    </tr>
    </thead>
    <tbody>
    <% @committees.each do |committee| %>
        <tr>
          <td><%= committee.name %></td>
          <td><%= link_to image_tag(@committee.image.url(:med)),
                          @committee.image.url %></td>
        </tr>
        <% end %>
    </tbody>
    <tfoot>

    </tfoot>
  </table>
</div>